{
  "name": "geometr",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/main.umd.js",
  "module": "dist/main.es.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "require": "./dist/main.umd.js",
      "import": "./dist/main.es.js"
    },
    "./styles": {
      "require": "./dist/styles.umd.js",
      "import": "./dist/styles.es.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "bump!": "sh scripts/bump.sh",
    "build": "tsc --project tsconfig.build.json --emitDeclarationOnly && vite build",
    "lint:eslint": "eslint --ext .js,.ts,.tsx,.vue .",
    "lint:stylelint": "stylelint **/*.{css,scss,vue}",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "update:storybook": "storybook upgrade --prerelease",
    "version:get": "node -p \"require('./package.json').version\""
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.0-beta.46",
    "@storybook/addon-backgrounds": "^7.0.0-beta.46",
    "@storybook/addon-docs": "^7.0.0-beta.46",
    "@storybook/addon-essentials": "^7.0.0-beta.46",
    "@storybook/addon-links": "^7.0.0-beta.46",
    "@storybook/addon-measure": "^7.0.0-beta.46",
    "@storybook/addon-outline": "^7.0.0-beta.46",
    "@storybook/blocks": "7.0.0-beta.46",
    "@storybook/channel-postmessage": "^7.0.0-beta.46",
    "@storybook/channel-websocket": "^7.0.0-beta.46",
    "@storybook/client-api": "^7.0.0-beta.46",
    "@storybook/client-logger": "^7.0.0-beta.46",
    "@storybook/preview-api": "7.0.0-beta.46",
    "@storybook/vue3": "^7.0.0-beta.46",
    "@storybook/vue3-vite": "^7.0.0-beta.46",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-vue": "^9.9.0",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.57.1",
    "storybook": "^7.0.0-beta.46",
    "stylelint": "^15.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.11"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  }
}
