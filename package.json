    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.4.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.3",
    "sass": "^1.49.7",
    "stylelint": "^14.5.1",
