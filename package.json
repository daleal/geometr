{
  "name": "geometr",
  "version": "0.3.1",
  "type": "module",
  "main": "dist/main.cjs.js",
  "module": "dist/main.es.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "require": "./dist/main.cjs.js",
      "import": "./dist/main.es.js"
    },
    "./components": {
      "require": "./dist/components.cjs.js",
      "import": "./dist/components.es.js"
    },
    "./composables": {
      "require": "./dist/composables.cjs.js",
      "import": "./dist/composables.es.js"
    },
    "./styles": {
      "require": "./dist/style.css",
      "import": "./dist/style.css"
    }
  },
  "sideEffects": false,
  "scripts": {
    "bump!": "sh scripts/bump.sh",
    "build": "vite build && tsc --project tsconfig.build.json --emitDeclarationOnly",
    "lint:eslint": "eslint --ext .js,.ts,.tsx,.vue .",
    "lint:stylelint": "stylelint **/*.{css,scss,vue}",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "update:storybook": "storybook upgrade --prerelease",
    "version:get": "node -p \"require('./package.json').version\""
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@fontsource/chakra-petch": "^4.5.9",
    "@storybook/addon-actions": "^7.0.23",
    "@storybook/addon-backgrounds": "^7.0.23",
    "@storybook/addon-docs": "^7.0.23",
    "@storybook/addon-essentials": "^7.0.23",
    "@storybook/addon-links": "^7.0.23",
    "@storybook/addon-measure": "^7.0.23",
    "@storybook/addon-outline": "^7.0.23",
    "@storybook/blocks": "7.0.0-beta.46",
    "@storybook/channel-postmessage": "^7.0.23",
    "@storybook/channel-websocket": "^7.0.23",
    "@storybook/client-api": "^7.0.23",
    "@storybook/client-logger": "^7.0.23",
    "@storybook/preview-api": "7.0.0-beta.46",
    "@storybook/vue3": "^7.0.23",
    "@storybook/vue3-vite": "^7.0.23",
    "@types/node": "^18.16.18",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vueuse/core": "^10.2.0",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.15.1",
    "postcss": "^8.4.24",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.63.6",
    "storybook": "^7.0.23",
    "stylelint": "^15.9.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-scss": "^5.0.1",
    "typescript": "^4.9.5",
    "vite": "^4.3.9",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.1"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  }
}
